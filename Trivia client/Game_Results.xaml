        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Trivia_client"
        mc:Ignorable="d"
        Title="Game_Results" Height="450" Width="400">
    <Grid>
        <Label Content="Best Results" FontSize="22" HorizontalAlignment="Left" Margin="138,39,0,0" VerticalAlignment="Top"/>

        <Button Content="Ok"
                FontSize="22"
                HorizontalAlignment="Left"
                Margin="150,340,0,0"
                VerticalAlignment="Top"
                Click="Continue"
                Width="100"/>

        <ListBox Name ="results_list" HorizontalAlignment="Left" Height="215" Margin="86,95,0,0" VerticalAlignment="Top" Width="224"/>



    </Grid>
</Window>
